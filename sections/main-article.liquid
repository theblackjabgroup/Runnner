<script>
  document.addEventListener('DOMContentLoaded', function() {
  const headingEl = document.querySelector('.font-medium.uppercase.leading-none.tracking-tighter.text-left .animated-text') || 
                   document.querySelector('.animated-text');
  
  if (headingEl) {
    const text = headingEl.innerText;
    let html = '';
    for (let i = 0; i < text.length; i++) {
      const char = text[i] === ' ' ? '&nbsp;' : text[i];
      const delay = i * 0.07;
      html += `<span class="letter" style="animation-delay: ${delay}s;">${char}</span>`;
    }
    
    headingEl.innerHTML = html;
    
    const lastLetterDelay = (text.length - 1) * 0.07;
    const textAnimationCompleteTime = lastLetterDelay + 0.8; 
    
    const dateEl = document.querySelector('.date-tag');
    if (dateEl) {
      dateEl.style.animationDelay = `${textAnimationCompleteTime + 0.2}s`;
    }
    const arrowEl = document.querySelector('svg');
    if (arrowEl) {
      arrowEl.style.animationDelay = `${textAnimationCompleteTime + 0.4}s`;
    }

    const parallaxImage = document.querySelector('.parallax-image-overlay img');
    if (parallaxImage) {
      parallaxImage.style.animation = 'none';
      parallaxImage.style.opacity = '0';
      parallaxImage.style.transform = 'translateY(40px)';
      
      setTimeout(() => {
        parallaxImage.style.transition = 'opacity 1.2s ease-out, transform 1.2s ease-out';
        parallaxImage.style.opacity = '1';
        parallaxImage.style.transform = 'translateY(0)';
        
        console.log(`Parallax fade-in triggered after ${textAnimationCompleteTime + 0.6} seconds`);
      }, (textAnimationCompleteTime) * 1000);
    }
  }

  const overlay = document.querySelector('.overlay-content');
  const OFFSET_TOP = 55;
  let ticking = false;
  let isMobile = false;
  let isTablet = false;
  let lastScrollY = 0;

  function detectDevice() {
    const width = window.innerWidth;
    const userAgent = navigator.userAgent.toLowerCase();
    const isTouchDevice = 'ontouchstart' in window || navigator.maxTouchPoints > 0;
    
    isMobile = width <= 768;
    isTablet = width > 768 && width <= 1024 && isTouchDevice;
    
    document.body.className = document.body.className.replace(/\b(mobile|tablet|desktop)\b/g, '');
    if (isMobile) {
      document.body.classList.add('mobile');
    } else if (isTablet) {
      document.body.classList.add('tablet');
    } else {
      document.body.classList.add('desktop');
    }
    return { isMobile, isTablet };
  }

  const deviceInfo = detectDevice();
  isMobile = deviceInfo.isMobile;
  isTablet = deviceInfo.isTablet;

  function requestTick() {
    if (!ticking) {
      requestAnimationFrame(onScroll);
      ticking = true;
    }
  }

  function onScroll() {
    ticking = false;
    
    if (!overlay) 
      return;
    const container = document.querySelector('.parallax-container');
    if (!container) 
      return;
    const containerRect = container.getBoundingClientRect();
    const overlayHeight = overlay.offsetHeight;
    const viewportHeight = window.innerHeight;
    const currentScrollY = window.pageYOffset || document.documentElement.scrollTop;
    const startParallax = containerRect.top <= OFFSET_TOP;
    const stopPoint = containerRect.bottom - overlayHeight - OFFSET_TOP;
    const shouldStopParallax = stopPoint <= OFFSET_TOP;
    overlay.classList.remove('mobile-fixed', 'mobile-absolute');
    if (startParallax && !shouldStopParallax) {
      overlay.style.position = 'fixed';
      overlay.style.top = `${OFFSET_TOP}px`;
      overlay.style.zIndex = '10';
      overlay.style.transform = 'translateZ(0)';
      if (isMobile) {
         overlay.classList.add('mobile-fixed');
        overlay.style.left = '0';
        overlay.style.width = '100vw';
        overlay.style.maxWidth = '100vw';
        overlay.style.paddingLeft = '10px';
        overlay.style.paddingRight = '10px';
      } else {
        overlay.style.left = `${Math.max(0, containerRect.left)}px`;
        overlay.style.width = `${containerRect.width}px`;
        overlay.style.paddingLeft = '';
        overlay.style.paddingRight = '';
        overlay.style.maxWidth = '';
      }
    } else if (shouldStopParallax) {
      overlay.style.position = 'absolute';
      overlay.style.top = `${Math.max(0, container.offsetHeight - overlayHeight-55)}px`;
      overlay.style.zIndex = '';
      overlay.style.transition = 'none';
      overlay.style.transform = 'translateY(-20px)';
      overlay.style.opacity = '1';

      requestAnimationFrame(() => {
        overlay.style.opacity = '1';
      });
      
      if (isMobile) {
        overlay.classList.add('mobile-absolute');
        overlay.style.paddingLeft = '10px';
        overlay.style.paddingRight = '10px';
        overlay.style.maxWidth = '100vw';
      } else {
        overlay.style.paddingLeft = '';
        overlay.style.paddingRight = '';
        overlay.style.maxWidth = '';
      }
    } else {
      overlay.style.position = 'relative';
      overlay.style.top = '';
      overlay.style.left = '';
      overlay.style.width = '';
      overlay.style.zIndex = '';
      overlay.style.transform = '';
      overlay.style.paddingLeft = '';
      overlay.style.paddingRight = '';
      overlay.style.maxWidth = '';
    }
    if (isMobile) {
      const image = document.querySelector('.parallax-image-overlay img');
      if (image) {
        const scrollDistance = window.scrollY;
      }
    }
    lastScrollY = currentScrollY;
  }

  let resizeTimeout;
  let orientationTimeout;
  
  function handleResize() {
    clearTimeout(resizeTimeout);
    resizeTimeout = setTimeout(() => {
      const newDeviceInfo = detectDevice();
      isMobile = newDeviceInfo.isMobile;
      isTablet = newDeviceInfo.isTablet;
      requestTick();
    }, 16);
  }
  function handleOrientationChange() {
    clearTimeout(orientationTimeout);
    orientationTimeout = setTimeout(() => {
      const newDeviceInfo = detectDevice();
      isMobile = newDeviceInfo.isMobile;
      isTablet = newDeviceInfo.isTablet;
      requestTick();
    }, 100); 
  }
  const scrollOptions = { passive: true };
  const resizeOptions = { passive: true };
  window.addEventListener('scroll', requestTick, scrollOptions);
  window.addEventListener('resize', handleResize, resizeOptions);
  if (isMobile || isTablet) {
    window.addEventListener('touchmove', requestTick, scrollOptions);
    window.addEventListener('orientationchange', handleOrientationChange, resizeOptions);
    
    let lastTouchEnd = 0;
    document.addEventListener('touchend', function (event) {
      const now = (new Date()).getTime();
      if (now - lastTouchEnd <= 300) {
        event.preventDefault();
      }
      lastTouchEnd = now;
    }, false);
    let scrollTimer = null;
    window.addEventListener('scroll', function() {
      if (scrollTimer !== null) {
        clearTimeout(scrollTimer);        
      }
      scrollTimer = setTimeout(function() {
        document.body.style.pointerEvents = 'auto';
      }, 150);
    }, scrollOptions);
  }
  requestTick();
  
  const parallaxImage = document.querySelector('.parallax-image-overlay img');
  if (parallaxImage && parallaxImage.src) {
    const img = new Image();
    img.src = parallaxImage.src;
  }
  if (isMobile && 'performance' in window) {
    const observer = new PerformanceObserver((list) => {
      for (const entry of list.getEntries()) {
        if (entry.entryType === 'measure' && entry.duration > 16) {
          console.warn('Performance issue detected:', entry.name, entry.duration);
        }
      }
    });
    
    try {
      observer.observe({ entryTypes: ['measure'] });
    } catch (e) {
      console.log('Performance observer not supported');
    }
  }
});
</script>
